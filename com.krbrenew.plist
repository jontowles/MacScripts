<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.krbrenew.daemon</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/krenew.sh</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StartInterval</key>
	<integer>600</integer>
	<key>StandardErrorPath</key>
	<string>/usr/local/com.krbnew.daemon.err</string>
	<key>StandardOutPath</key>
	<string>/usr/local/com.krbnew.daemon.out</string>
</dict>
</plist>
